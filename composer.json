{
    "name": "molajo/language",
    "type": "molajo-package",
    "description": "Molajo Language Services for PHP Applications",
    "version": "1.0.0",
    "keywords": ["Language", "CommonApi", "Molajo"],
    "homepage": "http://github.com/Molajo/Language",
    "license": "MIT",
    "authors": [
        {
            "name": "Amy Stephen",
            "email": "AmyStephen@Molajo.org",
            "homepage": "http://AmyStephen.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "commonapi/language": "dev-master",
        "commonapi/database": "dev-master",
        "commonapi/query": "dev-master"
    },
    "recommend": {
        "molajo/query": "dev-master"
    },
    "support": {
        "event": "hello@molajo.org",
        "irc": "irc://irc.freenode.org/molajo"
    },
    "autoload": {
        "psr-4": {
            "Molajo\\Language\\": "Source/",
            "Molajo\\Factories\\": "Factories/"
        }
    },

    "archive": {
        "exclude": ["/.dev"]
    },

    "minimum-stability": "dev"
}
